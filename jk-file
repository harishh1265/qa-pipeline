pipeline {
			agent any
			stages {
				stage('BUILD') {
					steps {
						git branch: 'main', url: 'https://github.com/harishh1265/java-codes.git'
							sleep 5
					}
				}
				stage('DEPLOY') {
					steps {
						sh 'echo "Final DEPLOY stage in Jenkinsfile"; sleep 5'	
					}
				}
				stage('STATUS') {
					steps {
						sh 'echo "Pipeline job completed"; sleep 5'	
					}
				}
			}
		}
